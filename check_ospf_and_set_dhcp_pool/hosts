[ios]
R1 ansible_host=192.168.1.1
R2 ansible_host=192.168.1.2
R3 ansible_host=192.168.3.1
R4 ansible_host=192.168.4.2

[ios:vars]
ansible_user=admin
ansible_password=cisco
ansible_connection=network_cli
ansible_network_os=ios

R1 ansible_host=192.168.1.1 ansible_user=admin ansible_password=cisco ansible_connection=network_cli ansible_network_os=ios
R2 ansible_host=192.168.1.2 ansible_user=admin ansible_password=cisco ansible_connection=network_cli ansible_network_os=ios
R3 ansible_host=192.168.3.1 ansible_user=admin ansible_password=cisco ansible_connection=network_cli ansible_network_os=ios
R4 ansible_host=192.168.4.2 ansible_user=admin ansible_password=cisco ansible_connection=network_cli ansible_network_os=ios
